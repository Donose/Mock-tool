{
  "manifest_version": 3,
  "name": "Mock Redirect Extension",
  "version": "1.1",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:4000/*",
    "*://uat-public-ubiservices.ubi.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
